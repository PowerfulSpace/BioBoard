<!DOCTYPE html>
<html lang="en">
<head>
   @await Html.PartialAsync("Partial/HeadPartial/MetatagsPartial")
    @await Html.PartialAsync("Partial/HeadPartial/CssPartial")
</head>
<body>
    <header>
      @await Html.PartialAsync("Partial/BodyPartial/HeaderPartial")
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="mt-5 pt-4 border-top text-center">
        @await Html.PartialAsync("Partial/BodyPartial/FooterPartial")
    </footer>
    @await Html.PartialAsync("Partial/BodyPartial/ScriptsPartial")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
